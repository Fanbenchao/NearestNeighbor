class NearestNeighbor:
  def __init__(self):
    pass
  def train(self,X,y):
    self.Xtr = X
    self.ytr = y
  def predict(self,X):
    num_test = X.shape[0]
    Ypred = np.zeros(num_test,dtype = self.ytr.dtype)
    for i in range(num_test):
      distence = np.sum(np.abs(X-X[i,:]),axis = 1)
      num_index = np.argmin(distence)
      Ypred[i] = self.ytr[num_index]
    return Ypred
